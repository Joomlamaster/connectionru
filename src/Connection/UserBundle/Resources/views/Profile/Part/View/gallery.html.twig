{% if userPhotos is defined and userPhotos is not empty %}
    {% for gallery in userPhotos %}
        {% for galleryName, images in gallery %}
            <div class="row profile-gallery">
                {% for image in images %}
                    <div class="user-profile-image-box">
                        <img src="{{ image.path }}" data-image_id="{{ image.id }}">
                    </div>
                {% endfor %}
            </div>
        {% endfor %}
    {% endfor %}
{% endif %}